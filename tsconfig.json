{
  "compilerOptions": {
    "baseUrl": "./",
    "allowJs": true,
    "isolatedModules":false,
    "types": ["element-plus/global"],
    "lib": ["dom","es5","ES2015"],
    "paths": {
      "@/*": ["./src/*"]
    },
  },
  "exclude": ["node_modules", "dist"],
  "include": [
    "typed-router.d.ts",
    "components.d.ts",
    "auto-imports.d.ts",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue"
  ],
  "extends": "@vue/tsconfig/tsconfig.json"
}
